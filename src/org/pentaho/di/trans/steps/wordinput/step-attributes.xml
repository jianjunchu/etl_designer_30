  <attribute id="FIELD_NAME"> <xmlcode>name</xmlcode>  <repcode>field_name</repcode> <description>WordInputDialog.NameColumn.Column</description> <tooltip/> <valuetype>String</valuetype> <parentid>FIELD</parentid> </attribute>
  <attribute id="FIELD_TYPE"> <xmlcode>type</xmlcode>  <repcode>field_type</repcode> <description>WordInputDialog.TypeColumn.Column</description> <tooltip/> <valuetype>String</valuetype> <parentid>FIELD</parentid> </attribute>
  <attribute id="FIELD_FORMAT"> <xmlcode>format</xmlcode>  <repcode>field_format</repcode> <description>WordInputDialog.FormatColumn.Column</description> <tooltip/> <valuetype>String</valuetype> <parentid>FIELD</parentid> </attribute>
  <attribute id="FIELD_LENGTH"> <xmlcode>length</xmlcode>  <repcode>field_length</repcode> <description>WordInputDialog.LengthColumn.Column</description> <tooltip/> <valuetype>String</valuetype> <parentid>FIELD</parentid> </attribute>
  <attribute id="FIELD_PRECISION"> <xmlcode>precision</xmlcode>  <repcode>field_precision</repcode> <description>WordInputDialog.PrecisionColumn.Column</description> <tooltip/> <valuetype>String</valuetype> <parentid>FIELD</parentid> </attribute>
