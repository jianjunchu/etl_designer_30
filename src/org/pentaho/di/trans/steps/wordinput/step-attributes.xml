  <attribute id="INCLUDE_FILENAME"> <xmlcode>include_filename</xmlcode>  <repcode/> <description>WordInputDialog.IncludeFilenameField.Label</description> <tooltip/> <valuetype>Boolean</valuetype> <parentid/></attribute>
  <attribute id="ROW_NUM_FIELD"> <xmlcode>rownum_field</xmlcode>  <repcode/> <description>WordInputDialog.RowNumField.Label</description> <tooltip/> <valuetype>String</valuetype> <parentid/></attribute>
  <attribute id="HEADER_PRESENT"> <xmlcode>header</xmlcode>  <repcode/> <description>WordInputDialog.HeaderPresent.Label</description> <tooltip/> <valuetype>Boolean</valuetype> <parentid/></attribute>
  <attribute id="START_ROW_INDEX"> <xmlcode>separator</xmlcode>  <repcode/> <description>WordInputDialog.Delimiter.Label</description> <tooltip/> <valuetype>String</valuetype> <parentid/></attribute>
  <attribute id="TABLE_NR"> <xmlcode>buffer_size</xmlcode>  <repcode/> <description>WordInputDialog.BufferSize.Label</description> <tooltip/> <valuetype>String</valuetype> <parentid/></attribute>
